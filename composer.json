{
  "name": "datashaman/tongs-feed",
  "type": "library",
  "description": "Feed plugin for Tongs",
  "license": "MIT",
  "authors": [
    {
      "name": "Marlin Forbes",
      "email": "marlinf@datashamab.com"
    }
  ],
  "require": {
    "datashaman/tongs": "^0.2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "tongs": {
      "plugins": {
        "feed": "Datashaman\\Tongs\\Plugins\\FeedPlugin"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Datashaman\\Tongs\\Plugins\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Datashaman\\Tongs\\Plugins\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
